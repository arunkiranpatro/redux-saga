{
    "name": "react-template",
    "version": "1.0.0",
    "description": "A react starter kit",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "start": "./node_modules/.bin/webpack-dev-server",
        "sass": "./node_modules/.bin/sass ./css/index.scss ./dist/styles.css",
        "lint": "eslint --ext .jsx,.js src "
    },
    "keywords": [],
    "author": "Arun Kiran Patro",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/arunkiranpatro/react-template.git"
    },
    "dependencies": {
        "array-sort": "^1.0.0",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "moment": "^2.24.0",
        "mongoose": "^5.9.1",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-icons": "^3.9.0",
        "react-moment": "^0.9.7",
        "react-redux": "^7.1.3",
        "redux": "^4.0.5",
        "redux-mock-store": "^1.5.4",
        "redux-saga": "^1.1.3",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.8.3",
        "@babel/preset-react": "^7.8.3",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.6",
        "compression-webpack-plugin": "^3.1.0",
        "css-loader": "^3.4.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.3",
        "jest": "^25.1.0",
        "jest-cli": "^25.1.0",
        "jest-html-reporter": "^2.8.0",
        "mini-css-extract-plugin": "^0.9.0",
        "moment-locales-webpack-plugin": "^1.1.2",
        "node-sass": "^4.13.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "redux-devtools-extension": "^2.13.8",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "url-loader": "^3.0.0",
        "webpack": "^5.0.0-beta.13",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>setupTests.js"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "collectCoverage": true,
        "testResultsProcessor": "./node_modules/jest-html-reporter"
    },
    "browserslist": {
        "development": [
            "last 2 chrome versions",
            "last 2 firefox versions",
            "last 2 edge versions"
        ],
        "production": [
            ">1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 11"
        ]
    }
}
